do
$$
    declare
        m_id integer;
        c_id integer;
    BEGIN
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理', {{module.menuId}},'/{{module.name}}/{{biz}}','{{module.name}}:{{biz}}','CATALOG') returning id into c_id;
        insert into sys_role_menu(role_id, menu_id) values (1, c_id);
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理:菜单', c_id,'','{{module.name}}:{{biz}}:menu','MENU') returning id into m_id;
        insert into sys_role_menu(role_id, menu_id) values (1, m_id);
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理:创建', c_id,'','{{module.name}}:{{biz}}:create','BUTTON') returning id into m_id;
        insert into sys_role_menu(role_id, menu_id) values (1, m_id);
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理:更新', c_id,'','{{module.name}}:{{biz}}:update','BUTTON') returning id into m_id;
        insert into sys_role_menu(role_id, menu_id) values (1, m_id);
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理:删除', c_id,'','{{module.name}}:{{biz}}:delete','BUTTON') returning id into m_id;
        insert into sys_role_menu(role_id, menu_id) values (1, m_id);
        insert into sys_menu(name, parent_id, path, permission, type) values ('{{comment}}管理:导出', c_id,'','{{module.name}}:{{biz}}:export','BUTTON') returning id into m_id;
        insert into sys_role_menu(role_id, menu_id) values (1, m_id);
    end
$$;
